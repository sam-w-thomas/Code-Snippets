import java.util.Scanner;

class Main
{
    public static void main(String[] args)
    {
        Scanner scanner = new Scanner(System.in);
        int reference = 0;

        while (true) {
            int sequence = scanner.nextInt();
            if (sequence != 0) {
                if (sequence > reference) {
                    reference = sequence;
                } else {
                    continue;
                }
            } else {
                break;
            }
        }

        System.out.println(reference);
    }
}
